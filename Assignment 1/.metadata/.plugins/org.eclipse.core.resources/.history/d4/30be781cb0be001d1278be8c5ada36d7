#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	int x=10;
	int y=5;
	int temp;
	printf("Before swapping:\n");
	printf("x= %d and y= %d\n",x,y);
	x=temp;
	x=y;
	y=temp;
	printf("After swapping:\n");
	printf("x= %d and y= %d\n",x,y);
}
